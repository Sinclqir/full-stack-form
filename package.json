  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:ui": "cypress run --spec 'cypress/e2e/ui.cy.js'",
